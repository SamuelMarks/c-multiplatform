@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if(@CMP_HAS_SDL3@)
    find_dependency(SDL3 CONFIG)
endif()

if(@CMP_HAS_SDL3_TTF@)
    find_dependency(SDL3_ttf CONFIG)
endif()

if(@CMP_HAS_GTK4@)
    find_dependency(PkgConfig)
    pkg_check_modules(GTK4 REQUIRED IMPORTED_TARGET gtk4)
endif()

if(@CMP_HAS_LIBCURL@)
    find_dependency(CURL)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/LibCMPCTargets.cmake")
include("${CMAKE_CURRENT_LIST_DIR}/LibM3Targets.cmake")
